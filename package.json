{
    "name": "cap-with-javascript-basics",
    "version": "1.0.0",
    "description": "A simple CAP project.",
    "repository": "<Add your repository here>",
    "license": "UNLICENSED",
    "private": true,
    "type": "module",
    "engines": {
        "node": ">=16"
    },
    "dependencies": {
        "@cap-js-community/odata-v2-adapter": "^1",
        "@cap-js/graphql": "^0.6.2",
        "@cloudnative/health-connect": "^2.1.0",
        "@sap/cds": "^7",
        "@sap/cds-common-content": "^1",
        "@sap/cds-fiori": "^1",
        "accept-language-parser": "^1.5.0",
        "body-parser": "^1.20.2",
        "cds-swagger-ui-express": "^0.6.0",
        "cors": "^2.8.5",
        "event-loop-lag": "^1.4.0",
        "express": "^4",
        "express-status-monitor": "^1.3.4",
        "glob": "^10.3.3",
        "helmet": "^7.0.0",
        "node-xlsx": "^0.23.0",
        "overload-protection": "^1.2.3",
        "swagger-jsdoc": "^6.2.8",
        "upath": "^2.0.1"
    },
    "devDependencies": {
        "@cap-js/cds-typer": "^0.5.0",
        "@cap-js/sqlite": "^1",
        "@sap/cds-dk": "^7",
        "@sap/eslint-plugin-cds": "^2",
        "eslint": "^8"
    },
    "scripts": {
        "watch": "cds watch --profile development",
        "start": "cds-serve",
        "types": "npx @cap-js/cds-typer ./db/schema.cds --outputDirectory ./@cds-models"
    },
    "cds": {
        "requires": {
            "auth": "mocked"
        },
        "log": {
            "levels": {
                "sqlite": "debug",
                "cds": "info",
                "nodejs": "info"
            }
        },
        "preview": {
            "ui5": {
                "version": "1.116.0"
            }
        },
        "cov2ap": {
            "plugin": true
        },
        "protocols": {
            "graphql": {
                "path": "/graphql",
                "impl": "@cap-js/graphql"
            }
        }
    },
    "imports": {
        "#cds-models/*": "./@cds-models/*/index.js"
    }
}